# ================================
# WORLDS AI BOT - FRONTEND SECRETS
# ================================
# Copy this file to .env and fill in your values
# All Vite env variables must start with VITE_

# =============
# API CONFIGURATION
# =============
# Backend API URL
VITE_BACKEND_URL=http://localhost:5001

# =============
# PAYMENT GATEWAY
# =============
# Razorpay Key ID (public key - safe to expose)
# Test mode keys start with 'rzp_test_'
# Live mode keys start with 'rzp_live_'
VITE_RAZORPAY_KEY=rzp_test_xxxxxxxxxxxx

# =============
# OPTIONAL: ANALYTICS
# =============
# Google Analytics ID
# VITE_GA_ID=G-XXXXXXXXXX

# =============
# OPTIONAL: OTHER SERVICES
# =============
# Add any other third-party service keys here
# VITE_SENTRY_DSN=https://xxx@sentry.io/xxx
